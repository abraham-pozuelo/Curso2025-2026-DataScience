  ] ;

  rr:predicateObjectMap [
    rr:predicate rdf:type ;
    rr:objectMap [ rr:constant bib:Direccion ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate bib:via ;
    rr:objectMap [ rml:reference "NOMBRE-VIA" ; rr:language "es" ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate bib:claseVial ;
    rr:objectMap [ rml:reference "CLASE-VIAL" ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate bib:numero ;
    rr:objectMap [ rml:reference "NUM" ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate bib:direccionCompleta ;
    rr:objectMap [ rml:reference "direccion_completa" ; rr:language "es" ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate bib:localidad ;
    rr:objectMap [ rml:reference "LOCALIDAD" ; rr:language "es" ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate bib:provincia ;
    rr:objectMap [ rml:reference "PROVINCIA" ; rr:language "es" ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate bib:codigoPostal ;
    rr:objectMap [ rml:reference "CODIGO-POSTAL" ]
  ] .
  


######################################################################
#   BARRIO
######################################################################

bib:TM_Barrio a rr:TriplesMap ;
  rml:logicalSource <#Source> ;

  rr:subjectMap [
    rr:template "https://group15.linkeddata.es/resource/barrio/{barrio_slug}" ;
  ] ;

  rr:predicateObjectMap [
    rr:predicate rdf:type ;
    rr:objectMap [ rr:constant bib:Barrio ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate bib:nombreBarrio ;
    rr:objectMap [ rml:reference "BARRIO" ; rr:language "es" ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate owl:sameAs ;
    rr:objectMap [ rml:reference "Barrios_with_links" ; rr:termType rr:IRI ]
  ] .
  


######################################################################
#   DISTRITO
######################################################################

bib:TM_Distrito a rr:TriplesMap ;
  rml:logicalSource <#Source> ;

  rr:subjectMap [
    rr:template "https://group15.linkeddata.es/resource/distrito/{distrito_slug}" ;
  ] ;

  rr:predicateObjectMap [
    rr:predicate rdf:type ;
    rr:objectMap [ rr:constant bib:Distrito ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate bib:nombreDistrito ;
    rr:objectMap [ rml:reference "DISTRITO" ; rr:language "es" ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate owl:sameAs ;
    rr:objectMap [ rml:reference "Distritos_with_links" ; rr:termType rr:IRI ]
  ] .
