@prefix mod: <https://w3id.org/mod#> .
@prefix ns: <http://movimientosostenible.org/group14#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns: a owl:Ontology ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

# CLASES

ns:Accidente a owl:Class ;
    rdfs:label "Accidente" .

ns:Bicicleta a owl:Class ;
    rdfs:label "Bicicleta" ;
    rdfs:subClassOf ns:TipoVehiculo .

ns:BicicletaEPAC a owl:Class ;
    rdfs:label "Bicicleta E P A C" ;
    rdfs:subClassOf ns:TipoVehiculo .

ns:Coordenadas a owl:Class ;
    rdfs:label "Coordenadas" .

ns:CruceSemaforoBici a owl:Class ;
    rdfs:label "Cruce Semáforo Bici" .

ns:Distrito a owl:Class ;
    rdfs:label "Distrito" .

ns:Fecha a owl:Class ;
    rdfs:label "Fecha" .

ns:Lesividad a owl:Class ;
    rdfs:label "Lesividad" .

ns:Localizacion a owl:Class ;
    rdfs:label "Localización" .

ns:Persona a owl:Class ;
    rdfs:label "Persona" .

ns:PersonaImplicadaAccidente a owl:Class ;
    rdfs:label "Persona Implicada Accidente" ;
    rdfs:subClassOf ns:Persona .

ns:Test a owl:Class ;
    rdfs:label "Test" .

ns:TestSustancias a owl:Class ;
    rdfs:label "Test Sustancias" ;
    rdfs:subClassOf ns:Test .

ns:TipoSemaforo a owl:Class ;
    rdfs:label "Tipo Semaforo" .

ns:TipoVehiculo a owl:Class ;
    rdfs:label "Tipo Vehiculo" .


# INDIVIDUOS

ns:2200PeatonBici a ns:TipoSemaforo ;
    rdfs:label "2200 Peaton Bici" .

ns:2_200Bici a ns:TipoSemaforo ;
    rdfs:label "2_200 Bici" .

ns:3100Bici a ns:TipoSemaforo ;
    rdfs:label "3100 Bici" .

ns:3200Bici a ns:TipoSemaforo ;
    rdfs:label "3200 Bici" .


# OBJECT PROPERTIES

ns:enDistrito a owl:ObjectProperty ;
    rdfs:label "en distrito" ;
    rdfs:range ns:Distrito ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf (
            ns:Accidente
            ns:CruceSemaforoBici
        )
    ] .

ns:hasDate a owl:ObjectProperty ;
    rdfs:label "has date" ;
    rdfs:domain ns:Accidente ;
    rdfs:range ns:Fecha .

ns:hasLesi a owl:ObjectProperty ;
    rdfs:label "has lesi" ;
    rdfs:domain ns:Accidente ;
    rdfs:range ns:Lesividad .

ns:hasType a owl:ObjectProperty ;
    rdfs:label "has type" ;
    rdfs:domain ns:Accidente ;
    rdfs:range ns:TipoVehiculo .

ns:implicada a owl:ObjectProperty ;
    rdfs:label "implicada" ;
    rdfs:domain ns:Accidente ;
    rdfs:range ns:PersonaImplicadaAccidente .

ns:ocurreEn a owl:ObjectProperty ;
    rdfs:label "ocurre en" ;
    rdfs:domain ns:Accidente ;
    rdfs:range ns:Localizacion .

ns:requiredTest a owl:ObjectProperty ;
    rdfs:label "required test" ;
    rdfs:domain ns:Accidente ;
    rdfs:range ns:TestSustancias .

ns:tieneCoordenadas a owl:ObjectProperty ;
    rdfs:label "tiene coordenadas" ;
    rdfs:range ns:Coordenadas ; 
    rdfs:domain [ 
        a owl:Class ;
        owl:unionOf (
            ns:Localizacion
            ns:CruceSemaforoBici
        )
    ] .

ns:tieneFechaInstalacion a owl:ObjectProperty ;
    rdfs:label "tiene fecha instalacion" ;
    rdfs:domain ns:CruceSemaforoBici ;
    rdfs:range ns:Fecha .

ns:tieneTipoSemaforo a owl:ObjectProperty ;
    rdfs:label "tiene tipo semaforo" ;
    rdfs:domain ns:CruceSemaforoBici ;
    rdfs:range ns:TipoSemaforo .

# DATATYPE PROPERTIES 

ns:alcohol a owl:DatatypeProperty ;
    rdfs:label "alcohol" ;
    rdfs:domain ns:TestSustancias ;
    rdfs:range xsd:Boolean .

ns:distritoId a owl:DatatypeProperty ;
    rdfs:label "distrito id" ;
    rdfs:domain ns:Distrito ;
    rdfs:range xsd:integer .

ns:distritoNombre a owl:DatatypeProperty ;
    rdfs:label "distrito nombre" ;
    rdfs:domain ns:Distrito ;
    rdfs:range xsd:string .

ns:droga a owl:DatatypeProperty ;
    rdfs:label "droga" ;
    rdfs:domain ns:TestSustancias ;
    rdfs:range xsd:Boolean .

ns:edad a owl:DatatypeProperty ;
    rdfs:label "edad" ;
    rdfs:domain ns:PersonaImplicadaAccidente ;
    rdfs:range xsd:String .

ns:gender a owl:DatatypeProperty ;
    rdfs:label "gender" ;
    rdfs:domain ns:PersonaImplicadaAccidente ;
    rdfs:range xsd:String .

ns:hasCode a owl:DatatypeProperty ;
    rdfs:label "has code" ;
    rdfs:domain ns:Lesividad ;
    rdfs:range xsd:Integer .

ns:hasNumber a owl:DatatypeProperty ;
    rdfs:label "has number" ;
    rdfs:domain ns:Localizacion ;
    rdfs:range xsd:Integer .

ns:hasTime a owl:DatatypeProperty ;
    rdfs:label "has time" ;
    rdfs:domain ns:Accidente ;
    rdfs:range xsd:String .

ns:latitud a owl:DatatypeProperty ;
    rdfs:label "latitud" ;
    rdfs:domain ns:Coordenadas ;
    rdfs:range xsd:Float .

ns:longitud a owl:DatatypeProperty ;
    rdfs:label "longitud" ;
    rdfs:domain ns:Coordenadas ;
    rdfs:range xsd:Float .

ns:meteo a owl:DatatypeProperty ;
    rdfs:label "meteo" ;
    rdfs:domain ns:Accidente ;
    rdfs:range xsd:String .

ns:numeroExpediente a owl:DatatypeProperty ;
    rdfs:label "numero expediente" ;
    rdfs:domain ns:Accidente ;
    rdfs:range xsd:String .

ns:tieneCruceId a owl:DatatypeProperty ;
    rdfs:label "tiene cruce id" ;
    rdfs:domain ns:CruceSemaforoBici ;
    rdfs:range xsd:Integer .

ns:tieneSemaforoId a owl:DatatypeProperty ;
    rdfs:label "tiene semaforo id" ;
    rdfs:domain ns:CruceSemaforoBici ;
    rdfs:range xsd:Integer .

ns:tipoAccidente a owl:DatatypeProperty ;
    rdfs:label "tipo accidente" ;
    rdfs:domain ns:Accidente ;
    rdfs:range xsd:String .
