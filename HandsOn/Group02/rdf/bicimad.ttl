@prefix rr:   <http://www.w3.org/ns/r2rml#> .
@prefix rml:  <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:   <http://semweb.mmlab.be/ns/ql#> .
@prefix ns:   <https://datos.grupo0225-cdia.org/ontology/bicimad#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

<#MedicionDiariaMapping>
  rml:logicalSource [
      rml:source "bicimad.csv" ;
      rml:referenceFormulation ql:CSV ;
  ] ;

  rr:subjectMap [
      rr:template "https://datos.grupo0225-cdia.org/resource/bicimad/medicion/{fecha}" ;
      rr:class ns:MedicionDiaria ;
  ] ;

  rr:predicateObjectMap [
      rr:predicate ns:fecha ;
      rr:objectMap [
          rml:reference "fecha" ;
          rr:datatype xsd:date
      ] ;
  ] ;

  rr:predicateObjectMap [
      rr:predicate ns:horasTotalesDisponibilidadBicicletasEnAnclajes ;
      rr:objectMap [
          rml:reference "horasTotalesDeDisponibilidad" ;
          rr:datatype xsd:decimal
      ] ;
  ] ;

  rr:predicateObjectMap [
      rr:predicate ns:horasTotalesUsosBicicletas ;
      rr:objectMap [
          rml:reference "horasTotalesDeUso" ;
          rr:datatype xsd:decimal
      ] ;
  ] ;

  rr:predicateObjectMap [
      rr:predicate ns:mediaBicicletasDisponibles ;
      rr:objectMap [
          rml:reference "bicicletasMediasDisponibilidad" ;
          rr:datatype xsd:decimal
      ] ;
  ] ;

  rr:predicateObjectMap [
      rr:predicate ns:totalHorasServicioBicicletas ;
      rr:objectMap [
          rml:reference "horasTtotalesDeServicio" ;
          rr:datatype xsd:decimal
      ] ;
  ] ;

  rr:predicateObjectMap [
      rr:predicate ns:totalUsos ;
      rr:objectMap [
          rml:reference "totalUsoAlDia" ;
          rr:datatype xsd:integer
      ] ;
  ] ;

  rr:predicateObjectMap [
      rr:predicate ns:usosAbonadoAnual ;
      rr:objectMap [
          rml:reference "usoPorAbandonadosAnuales" ;
          rr:datatype xsd:integer
      ] ;
  ] ;

  rr:predicateObjectMap [
      rr:predicate ns:usosAbonadoOcasional ;
      rr:objectMap [
          rml:reference "usoPorAbandonadosOcasionales" ;
          rr:datatype xsd:integer
      ] ;
  ] .
